{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ],
  "env": {
    "DB_TYPE": "@db_type",
    "DB_HOST": "@db_host",
    "DB_PORT": "@db_port",
    "DB_USERNAME": "@db_username",
    "DB_PASSWORD": "@db_password",
    "DB_DATABASE": "@db_database",
    "SECRET_KEY": "@secret_key",
    "API_KEY": "@api_key",
    "DATABASE_URL": "@database_url"
  }
}
